type Query {
    allJourney: [Journey!]
    findJourneyById(id: String!): Journey
}

input JourneyInput {
    destination: String!
    price: String
    owner: String
}

type Journey {
    id: ID
    destination: String!
    price: String
    owner: String
}

type Mutation {
  createJourney(input: JourneyInput): Journey
  deleteJourney(id: ID): ID
}

schema{
    query: Query
    mutation: Mutation
}